    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{1535AF5B-6F98-4471-BB52-A5F8539ADB82}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT DS.IDSOLICITUD, SP.IDPACIENTE, DS.ANO, DS.IDPARAMETRO, SP.ANULADA,&#xA;CASE WHEN DS.IDGRUPO &lt;&gt; 0 THEN DS.IDGRUPO ELSE DS.IDPARAMETRO END AS IDGRUPO,&#xA;CAST(SP.FECHASOLICITUD AS  date) AS FECHASOLICITUD, CONVERT(VARCHAR(8), SP.FECHASOLICITUD, 108) AS HORASOLICITUD, &#xA;DS.USUARIOINGRESO,LTRIM(RTRIM(DS.RESULTADO)) AS RESULTADO, &#xA;DS.PATOLOGICO, DS.VALIDADO1, DS.USUARIOVALIDO1,CAST( DS.FECHAVALIDACION1 AS DATE) AS FECHAVALIDACION1,&#xA;CONVERT(VARCHAR(8), DS.FECHAVALIDACION1, 108) AS HORAVALIDACION1, DS.VALIDADO2, DS.USUARIOVALIDO2, &#xA;CAST( DS.FECHAVALIDACION2 AS DATE) AS FECHAVALIDACION2,   CONVERT(VARCHAR(8), DS.FECHAVALIDACION2, 108) AS HORAVALIDACION2, DS.DETENIDO,&#xA;DS.IDNORMALIDAD, DS.COMENTARIO AS COMENTARIO1,DS.CODCOMENTARIO,  DS.ANTIBIOGRAMA, DS.REPETICION, DS.MUESTRARECIBIDA,&#xA;DS.IDMUESTRA, SP.COMENTARIO, CAST(SP.D0 AS varchar) AS IDCENTROATENCION,&#xA;ISNULL(CONVERT(int,SP.D1),0) AS IDPLAN,&#xA;ISNULL(CONVERT(int,SP.D2),0) AS CODMEDICO, CAST(SP.D4 AS varchar) D4, CAST(SP.D5 AS varchar) D5, &#xA;CASE WHEN SP.D6 = 2 THEN 1 ELSE 0 END ESTADOEMBARAZO, &#xA;CAST(SP.D7 AS varchar) D7, CAST(SP.D8 AS varchar) D8, CAST(SP.D9 AS varchar) D9, SP.CONSECUTIVO ,CONCAT (SP.ANO, '-', SP.NUMEROSOLICITUD) CODIGOBARRAS,&#xA;REPLACE(REPLACE(REPLACE(convert(varchar(700), LTRIM(RTRIM(C.COMENTARIO))),CHAR(9),''),CHAR(10),''),CHAR(13),'') AS COMENTARIOTEXTO,&#xA;CASE WHEN SP.FECHASOLICITUD &lt;= PAC.FECHANACIMIENTO THEN STAGECOLCANDWH.dbo.fnAgnos(SP.FECHASOLICITUD,PAC.FECHANACIMIENTO)&#xA;ELSE STAGECOLCANDWH.dbo.fnAgnos(PAC.FECHANACIMIENTO,SP.FECHASOLICITUD) END AS EdadPaciente,&#xA;CASE WHEN PAC.IDSEXO = 1 THEN 'MASCULINO' WHEN PAC.IDSEXO = 2 THEN 'FEMENINO' ELSE 'AMBOS' END AS SexoPac,&#xA;CAST(SP.D3 AS BIT) AS URGENCIA,&#xA;CAST(DS.FECHAIMPRIMIO AS datE) AS FECHAIMPRIMIO, DS.USUARIOIMPRIMIO, CAST(DIMDIAG.VALOR AS varchar(6)) AS NombreDiagnostico,&#xA;H.RESULTADO AS PERFIL_SENS, &#xA;H.MEDICAMENTO, H.MICRO,CAST(K.NOMBRE AS varchar(200)) AS IPSOrigen, &#xA;CAST(DIMRAD.VALOR AS varchar(100)) AS IDRADICACION, &#xA;CAST(DIMORD.VALOR AS varchar(100)) AS NO_ORDENAMIENTO,&#xA;CAST(COMEN.COMENTARIO AS varchar(200)) AS COMEN_SOLICITUD,&#xA;CAST(DS.USUARIORESULTADO AS varchar(100)) AS USUARIO_RESULTADO,&#xA;IIF(DIMVIAJ.VALOR =1,'SI',IIF(DIMVIAJ.VALOR=2,'NO','--')) AS IDVIAJERO,&#xA;CAST(DIMNEV.VALOR AS varchar(50)) AS Nevera,&#xA;ROW_NUMBER() OVER(ORDER BY SP.IDPACIENTE) AS rowid&#xA;FROM LABORATORIO.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; SP&#xA;LEFT JOIN DIMENSIONES.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; DIMDIAG ON SP.IDSOLICITUD = DIMDIAG.IDSOLICITUD AND DIMDIAG.DIMENSION=19 AND DIMDIAG.ANO = &quot; + LEFT(@[User::V_ANOMES],4) + &quot;&#xA;LEFT JOIN DIMENSIONES.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; DIMRAD ON SP.IDSOLICITUD = DIMRAD.IDSOLICITUD AND DIMRAD.DIMENSION=17 AND DIMRAD.ANO = &quot; + LEFT(@[User::V_ANOMES],4) + &quot;&#xA;LEFT JOIN DIMENSIONES.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; DIMVIAJ ON SP.IDSOLICITUD = DIMVIAJ.IDSOLICITUD AND DIMVIAJ.DIMENSION=22 AND DIMVIAJ.ANO = &quot; + LEFT(@[User::V_ANOMES],4) + &quot;&#xA;LEFT JOIN DIMENSIONES.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; DIMNEV ON SP.IDSOLICITUD = DIMNEV.IDSOLICITUD AND DIMNEV.DIMENSION=23 AND DIMNEV.ANO = &quot; + LEFT(@[User::V_ANOMES],4) + &quot;&#xA;LEFT JOIN DIMENSIONES.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; DIMORD ON SP.IDSOLICITUD = DIMORD.IDSOLICITUD AND DIMORD.DIMENSION=13 AND DIMORD.ANO = &quot; + LEFT(@[User::V_ANOMES],4) + &quot;&#xA;INNER JOIN GENERAL.TB_PACIENTES PAC ON PAC.IDPACIENTE = SP.IDPACIENTE&#xA;INNER JOIN LABORATORIO.TB_DETSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot; DS ON  DS.IDSOLICITUD = SP.IDSOLICITUD&#xA;LEFT JOIN  LABORATORIO.TB_DETCOMENTARIOS&quot; + LEFT(@[User::V_ANOMES],4) + &quot; C ON DS.IDSOLICITUD = C.IDSOLICITUD AND DS.IDPARAMETRO = C.IDPARAMETRO&#xA;LEFT JOIN (&#xA;SELECT H.IDSOLICITUD, H.IDPARAMETRO, H.RESULTADO, &#xA;I.NOMBRE AS MEDICAMENTO, J.NOMBRE AS MICRO FROM LABORATORIO.TB_DETMICROBIOLOGIA&quot; + LEFT(@[User::V_ANOMES],4) + &quot; H &#xA;LEFT JOIN LABORATORIO.TB_ANTIBIOTICOS I ON I.IDANTIBIOTICO = H.IDANTIBIOGRAMA&#xA;LEFT JOIN LABORATORIO.TB_MICROORGANISMOS J ON J.IDMICROORGANISMO = H.IDMICROORGANISMO) H ON H.IDSOLICITUD = DS.IDSOLICITUD AND H.IDPARAMETRO = DS.IDPARAMETRO&#xA;LEFT JOIN LABORATORIO.TB_CABCOMENTARIOS&quot; + LEFT(@[User::V_ANOMES],4) + &quot; COMEN ON COMEN.ANO = DS.ANO AND COMEN.IDSOLICITUD = DS.IDSOLICITUD&#xA;LEFT JOIN DIMENSIONES.TB_LISTAS K ON K.IDLISTA = SP.D7 and K.LISTA = 'IPS'&#xA;WHERE SP.ANULADA = 0 AND CONVERT(VARCHAR(6), SP.FECHASOLICITUD, 112) = &quot; + @[User::V_ANOMES]"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="V_QUERY_SOURCE">
      <DTS:VariableValue
        DTS:DataType="8">SELECT DS.IDSOLICITUD, SP.IDPACIENTE, DS.ANO, DS.IDPARAMETRO, SP.ANULADA,
CASE WHEN DS.IDGRUPO &lt;&gt; 0 THEN DS.IDGRUPO ELSE DS.IDPARAMETRO END AS IDGRUPO,
CAST(SP.FECHASOLICITUD AS  date) AS FECHASOLICITUD, CONVERT(VARCHAR(8), SP.FECHASOLICITUD, 108) AS HORASOLICITUD, 
DS.USUARIOINGRESO,LTRIM(RTRIM(DS.RESULTADO)) AS RESULTADO, 
DS.PATOLOGICO, DS.VALIDADO1, DS.USUARIOVALIDO1,CAST( DS.FECHAVALIDACION1 AS DATE) AS FECHAVALIDACION1,
CONVERT(VARCHAR(8), DS.FECHAVALIDACION1, 108) AS HORAVALIDACION1, DS.VALIDADO2, DS.USUARIOVALIDO2, 
CAST( DS.FECHAVALIDACION2 AS DATE) AS FECHAVALIDACION2,   CONVERT(VARCHAR(8), DS.FECHAVALIDACION2, 108) AS HORAVALIDACION2, DS.DETENIDO,
DS.IDNORMALIDAD, DS.COMENTARIO AS COMENTARIO1,DS.CODCOMENTARIO,  DS.ANTIBIOGRAMA, DS.REPETICION, DS.MUESTRARECIBIDA,
DS.IDMUESTRA, SP.COMENTARIO, CAST(SP.D0 AS varchar) AS IDCENTROATENCION,
ISNULL(CONVERT(int,SP.D1),0) AS IDPLAN,
ISNULL(CONVERT(int,SP.D2),0) AS CODMEDICO, CAST(SP.D4 AS varchar) D4, CAST(SP.D5 AS varchar) D5, 
CASE WHEN SP.D6 = 2 THEN 1 ELSE 0 END ESTADOEMBARAZO, 
CAST(SP.D7 AS varchar) D7, CAST(SP.D8 AS varchar) D8, CAST(SP.D9 AS varchar) D9, SP.CONSECUTIVO ,CONCAT (SP.ANO, '-', SP.NUMEROSOLICITUD) CODIGOBARRAS,
REPLACE(REPLACE(REPLACE(convert(varchar(700), LTRIM(RTRIM(C.COMENTARIO))),CHAR(9),''),CHAR(10),''),CHAR(13),'') AS COMENTARIOTEXTO,
CASE WHEN SP.FECHASOLICITUD &lt;= PAC.FECHANACIMIENTO THEN STAGECOLCANDWH.dbo.fnAgnos(SP.FECHASOLICITUD,PAC.FECHANACIMIENTO)
ELSE STAGECOLCANDWH.dbo.fnAgnos(PAC.FECHANACIMIENTO,SP.FECHASOLICITUD) END AS EdadPaciente,
CASE WHEN PAC.IDSEXO = 1 THEN 'MASCULINO' WHEN PAC.IDSEXO = 2 THEN 'FEMENINO' ELSE 'AMBOS' END AS SexoPac,
CAST(SP.D3 AS BIT) AS URGENCIA,
CAST(DS.FECHAIMPRIMIO AS datE) AS FECHAIMPRIMIO, DS.USUARIOIMPRIMIO, CAST(DIMDIAG.VALOR AS varchar(6)) AS NombreDiagnostico,
H.RESULTADO AS PERFIL_SENS, 
H.MEDICAMENTO, H.MICRO,CAST(K.NOMBRE AS varchar(200)) AS IPSOrigen, 
CAST(DIMRAD.VALOR AS varchar(100)) AS IDRADICACION, 
CAST(DIMORD.VALOR AS varchar(100)) AS NO_ORDENAMIENTO,
CAST(COMEN.COMENTARIO AS varchar(200)) AS COMEN_SOLICITUD,
CAST(DS.USUARIORESULTADO AS varchar(100)) AS USUARIO_RESULTADO,
IIF(DIMVIAJ.VALOR =1,'SI',IIF(DIMVIAJ.VALOR=2,'NO','--')) AS IDVIAJERO,
CAST(DIMNEV.VALOR AS varchar(50)) AS Nevera,
ROW_NUMBER() OVER(ORDER BY SP.IDPACIENTE) AS rowid
FROM LABORATORIO.TB_CABSOLICITUD2022 SP
LEFT JOIN DIMENSIONES.TB_CABSOLICITUD2022 DIMDIAG ON SP.IDSOLICITUD = DIMDIAG.IDSOLICITUD AND DIMDIAG.DIMENSION=19 AND DIMDIAG.ANO = 2022
LEFT JOIN DIMENSIONES.TB_CABSOLICITUD2022 DIMRAD ON SP.IDSOLICITUD = DIMRAD.IDSOLICITUD AND DIMRAD.DIMENSION=17 AND DIMRAD.ANO = 2022
LEFT JOIN DIMENSIONES.TB_CABSOLICITUD2022 DIMVIAJ ON SP.IDSOLICITUD = DIMVIAJ.IDSOLICITUD AND DIMVIAJ.DIMENSION=22 AND DIMVIAJ.ANO = 2022
LEFT JOIN DIMENSIONES.TB_CABSOLICITUD2022 DIMNEV ON SP.IDSOLICITUD = DIMNEV.IDSOLICITUD AND DIMNEV.DIMENSION=23 AND DIMNEV.ANO = 2022
LEFT JOIN DIMENSIONES.TB_CABSOLICITUD2022 DIMORD ON SP.IDSOLICITUD = DIMORD.IDSOLICITUD AND DIMORD.DIMENSION=13 AND DIMORD.ANO = 2022
INNER JOIN GENERAL.TB_PACIENTES PAC ON PAC.IDPACIENTE = SP.IDPACIENTE
INNER JOIN LABORATORIO.TB_DETSOLICITUD2022 DS ON  DS.IDSOLICITUD = SP.IDSOLICITUD
LEFT JOIN  LABORATORIO.TB_DETCOMENTARIOS2022 C ON DS.IDSOLICITUD = C.IDSOLICITUD AND DS.IDPARAMETRO = C.IDPARAMETRO
LEFT JOIN (
SELECT H.IDSOLICITUD, H.IDPARAMETRO, H.RESULTADO, 
I.NOMBRE AS MEDICAMENTO, J.NOMBRE AS MICRO FROM LABORATORIO.TB_DETMICROBIOLOGIA2022 H 
LEFT JOIN LABORATORIO.TB_ANTIBIOTICOS I ON I.IDANTIBIOTICO = H.IDANTIBIOGRAMA
LEFT JOIN LABORATORIO.TB_MICROORGANISMOS J ON J.IDMICROORGANISMO = H.IDMICROORGANISMO) H ON H.IDSOLICITUD = DS.IDSOLICITUD AND H.IDPARAMETRO = DS.IDPARAMETRO
LEFT JOIN LABORATORIO.TB_CABCOMENTARIOS2022 COMEN ON COMEN.ANO = DS.ANO AND COMEN.IDSOLICITUD = DS.IDSOLICITUD
LEFT JOIN DIMENSIONES.TB_LISTAS K ON K.IDLISTA = SP.D7 and K.LISTA = 'IPS'
WHERE SP.ANULADA = 0 AND CONVERT(VARCHAR(6), SP.FECHASOLICITUD, 112) = 202202</DTS:VariableValue>
    </DTS:Variable>
  </DTS:Variables>
