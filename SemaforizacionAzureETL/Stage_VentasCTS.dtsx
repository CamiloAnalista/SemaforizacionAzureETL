                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select vta.IdVenta, CONVERT(VARCHAR(8), vta.FechaVenta, 112) fecha_creacion,  
vta.IdEstadoVenta,
CASE WHEN vta.NoFactura IS NULL THEN 'NA' ELSE vta.NoFactura END as numero_factura, 
CASE WHEN vta.IdSolicitudAthenea IS NULL THEN 0 ELSE vta.IdSolicitudAthenea END as id_solicitud_athenea,
pag.MedioPago, CAST(vta.FechaTransaccion as date) fecha_pago, SUBSTRING(pac.documento,1,20) as documento,
CAST(substring(vta.observacion,1,200) AS VARCHAR(200)) as observacion,
usu.NombreUsuario as usuario, usu.NombreCompleto as nombre_usuario,
vta.NumTransaccion,
exa.CodAthenea as Examen, 
ISNULL(det.PrecioVenta,0) as valor,
2 as tipo_examen,
vta.IdTipoServ
from ventas vta
left join Paciente pac on pac.Id_Paciente = vta.Id_Paciente
left join Ventas_Detalle det on det.IdVenta = vta.IdVenta
left join Examenes exa on exa.IdExamen = det.IdExamen
left join MediosPagos pag on pag.IdMedioPago = vta.IdMedioPago
left join Usuarios usu on usu.IdUsuario = vta.IdUsuario
where det.IdChequeo IS NULL
and cast(vta.FechaVenta as date) between cast(GETDATE()-20 as date) and cast(GETDATE() as date)
/*and cast(vta.FechaVenta as date)
between '2021-05-01' and '2021-06-30'*/

UNION

select vta.IdVenta,  CONVERT(VARCHAR(8), vta.FechaVenta, 112)  fecha_creacion,  
vta.IdEstadoVenta,
CASE WHEN vta.NoFactura IS NULL THEN 'NA' ELSE vta.NoFactura END as numero_factura, 
CASE WHEN vta.IdSolicitudAthenea IS NULL THEN 0 ELSE vta.IdSolicitudAthenea END as id_solicitud_athenea,
pag.MedioPago, CAST(vta.FechaTransaccion as date) fecha_pago, SUBSTRING(pac.documento,1,20) as documento,
CAST(vta.observacion AS VARCHAR(200)),
usu.NombreUsuario as usuario, usu.NombreCompleto as nombre_usuario,
vta.NumTransaccion,
cheq.CodAthenea as Examen, 
ISNULL(det.PrecioVenta,0) as valor,
1 as tipo_examen,
vta.IdTipoServ
from ventas vta
left join Paciente pac on pac.Id_Paciente = vta.Id_Paciente
left join Ventas_Detalle det on det.IdVenta = vta.IdVenta
left join Chequeos cheq on cheq.IdChequeo = det.IdChequeo
left join MediosPagos pag on pag.IdMedioPago = vta.IdMedioPago
left join Usuarios usu on usu.IdUsuario = vta.IdUsuario
where det.IdExamen IS NULL
and cast(vta.FechaVenta as date) between cast(GETDATE()-20 as date) and cast(GETDATE() as date)
/*and cast(vta.FechaVenta as date)
between '2021-11-01' and '2021-12-10'*/</property>
