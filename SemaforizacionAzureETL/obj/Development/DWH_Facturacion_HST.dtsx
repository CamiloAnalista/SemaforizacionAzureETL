    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A1FAD1CE-CD2D-4C57-A427-E0B1FFCCAF1B}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT DISTINCT C.IDSOLICITUD, C.ANO, C.IDDOCUMENTO, C.IDENTIDAD, C.FECHASOLICITUD, C.FECHADOCUMENTO, C.FECHAVENCIMIENTO, &#xA;CRAD.FECHARADICACION, CAST(C.IDPLAN AS INT) IDPLAN, CAST(C.CENTROATENCION AS INT) CENTROATENCION, --C.URGENCIA , &#xA;&#x9;COALESCE(DS.DETENIDO,0) AS APLAZADO, IDPACIENTE, C.IDITEM, C.NUMEROSOLICITUD, C.PRECIOITEM, C.CANTIDAD, &#xA;&#x9;C.ESTADOFACTURA, C.NUMERODOCUMENTO, CAST(C.IDRADICACION AS VARCHAR(100)) AS IDRADICACION&#xA;FROM (&#xA;&#x9;SELECT DISTINCT CS.IDSOLICITUD, DCF.ANO ,DCF.IDDOCUMENTO, CONVERT (INT,LTRIM(RTRIM(DCF.IDENTIDADORIGEN))) IDENTIDAD,&#xA;&#x9;&#x9;CONVERT(DATE,CS.FECHASOLICITUD,120) AS FECHASOLICITUD, &#xA;&#x9;&#x9;CONVERT(DATE,CA.FECHADOCUMENTO,120 ) AS FECHADOCUMENTO,&#xA;&#x9;&#x9;CONVERT(DATE,CA.FECHAVENCIMIENTO,120) AS  FECHAVENCIMIENTO ,&#xA;&#x9;&#x9;CS.D1 AS IDPLAN, CS.D0 AS CENTROATENCION, --ISNULL(UR.NOMBRE,'NO') AS URGENCIA,&#xA;&#x9;&#x9;CS.IDPACIENTE, DCF.IDITEM, CONCAT(CS.ANO,'-',CS.NUMEROSOLICITUD) AS NUMEROSOLICITUD,ISNULL( DCF.PRECIOITEM,0) PRECIOITEM,&#xA;&#x9;&#x9;ISNULL(DCF.CANTIDAD,0) CANTIDAD, ISNULL(CA.DESCUENTO1,0) DESCUENTO1,ISNULL (CA.IVA,0) IVA,ISNULL(DCF.VALORCOPAGO,0) AS COPAGO,&#xA;&#x9;&#x9;ISNULL(DCF.VALORCM,0) AS VALORCM  , ISNULL(EF.NOMBRE, 'NO GENERADA') AS ESTADOFACTURA, &#xA;&#x9;&#x9; ISNULL(CONVERT (VARCHAR(22),RTRIM(LTRIM (REPLACE(CA.NUMERODOCUMENTO, '--','')))),'NA') AS NUMERODOCUMENTO,&#xA;&#x9;&#x9;ISNULL(UPPER(CA.CONCEPTO),'NA') AS CONCEPTO, ISNULL (CA.FACTURAPARTICULAR,'0') FACTURAPARTICULAR, 'ATHENEA' AS FUENTE,&#xA;&#x9;&#x9;SUBSTRING(CS1.VALOR,1,100) as IDRADICACION&#xA;                            &#xA;&#x9;FROM FACTURACION.TB_DISTRIBUCIONCUENTAFACTURADO&quot; + LEFT(@[User::V_ANOMES],4) + &quot; AS DCF&#xA;&#xA;&#x9;&#x9;INNER JOIN LABORATORIO.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot;   CS  WITH(NOLOCK) ON CS.IDSOLICITUD = DCF.IDORIGEN  AND CS.ANO = DCF.ANO AND CS.ANULADA = 0&#xA;&#x9;&#x9;LEFT JOIN DIMENSIONES.TB_CABSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot;  CS1 WITH(NOLOCK) ON CS1.DIMENSION = 17 AND CS1.ANO = CS.ANO AND CS.IDSOLICITUD = CS1.IDSOLICITUD&#xA;&#x9;&#x9;--LEFT JOIN DIMENSIONES.VW_URGENTE  UR WITH(NOLOCK)  ON CS.CD3 = UR.IDLISTA&#xA;&#x9;&#x9;LEFT JOIN FACTURACION.TB_CABINGRESOS&quot; + LEFT(@[User::V_ANOMES],4) + &quot;  CA  WITH(NOLOCK) ON DCF.IDDOCUMENTO = CA.IDDOCUMENTO &#xA;&#x9;&#x9;LEFT JOIN  FACTURACION.TB_ESTADOFACTURA EF  WITH(NOLOCK) ON EF.IDESTADO = CA.ESTADO&#xA;&#x9;) C&#xA;LEFT JOIN LABORATORIO.TB_DETSOLICITUD&quot; + LEFT(@[User::V_ANOMES],4) + &quot;  DS WITH(NOLOCK) ON DS.IDSOLICITUD = C.IDSOLICITUD AND DS.ANO = C.ANO &#xA;&#x9;AND DS.IDPARAMETRO = (SELECT MAX(PA.IDPARAMETRO) FROM LABORATORIO.TB_PARAMETROS PA WHERE C.IDITEM = PA.CODIGOFACTURACION)&#xA;LEFT JOIN CARTERA.TB_DETRADICACION&quot; + LEFT(@[User::V_ANOMES],4) + &quot;  DRAD ON DRAD.IDDOCUMENTO = C.IDDOCUMENTO and ANULADO = 0&#xA;LEFT JOIN CARTERA.TB_CABRADICACION&quot; + LEFT(@[User::V_ANOMES],4) + &quot;  CRAD on CRAD.IDRADICACION = DRAD.IDRADICACION&#xA;&#xA;WHERE CONVERT(VARCHAR(6), C.FECHASOLICITUD, 112) = &quot; + @[User::V_ANOMES] + &quot; /*AND C.IDPLAN IN ('7281','7280','16712')*/&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="V_QUERY_SOURCE">
      <DTS:VariableValue
        DTS:DataType="8">SELECT DISTINCT C.IDSOLICITUD, C.ANO, C.IDDOCUMENTO, C.IDENTIDAD, C.FECHASOLICITUD, C.FECHADOCUMENTO, C.FECHAVENCIMIENTO, 
CRAD.FECHARADICACION, CAST(C.IDPLAN AS INT) IDPLAN, CAST(C.CENTROATENCION AS INT) CENTROATENCION, --C.URGENCIA , 
	COALESCE(DS.DETENIDO,0) AS APLAZADO, IDPACIENTE, C.IDITEM, C.NUMEROSOLICITUD, C.PRECIOITEM, C.CANTIDAD, 
	C.ESTADOFACTURA, C.NUMERODOCUMENTO, CAST(C.IDRADICACION AS VARCHAR(100)) AS IDRADICACION
FROM (
	SELECT DISTINCT CS.IDSOLICITUD, DCF.ANO ,DCF.IDDOCUMENTO, CONVERT (INT,LTRIM(RTRIM(DCF.IDENTIDADORIGEN))) IDENTIDAD,
		CONVERT(DATE,CS.FECHASOLICITUD,120) AS FECHASOLICITUD, 
		CONVERT(DATE,CA.FECHADOCUMENTO,120 ) AS FECHADOCUMENTO,
		CONVERT(DATE,CA.FECHAVENCIMIENTO,120) AS  FECHAVENCIMIENTO ,
		CS.D1 AS IDPLAN, CS.D0 AS CENTROATENCION, --ISNULL(UR.NOMBRE,'NO') AS URGENCIA,
		CS.IDPACIENTE, DCF.IDITEM, CONCAT(CS.ANO,'-',CS.NUMEROSOLICITUD) AS NUMEROSOLICITUD,ISNULL( DCF.PRECIOITEM,0) PRECIOITEM,
		ISNULL(DCF.CANTIDAD,0) CANTIDAD, ISNULL(CA.DESCUENTO1,0) DESCUENTO1,ISNULL (CA.IVA,0) IVA,ISNULL(DCF.VALORCOPAGO,0) AS COPAGO,
		ISNULL(DCF.VALORCM,0) AS VALORCM  , ISNULL(EF.NOMBRE, 'NO GENERADA') AS ESTADOFACTURA, 
		 ISNULL(CONVERT (VARCHAR(22),RTRIM(LTRIM (REPLACE(CA.NUMERODOCUMENTO, '--','')))),'NA') AS NUMERODOCUMENTO,
		ISNULL(UPPER(CA.CONCEPTO),'NA') AS CONCEPTO, ISNULL (CA.FACTURAPARTICULAR,'0') FACTURAPARTICULAR, 'ATHENEA' AS FUENTE,
		SUBSTRING(CS1.VALOR,1,100) as IDRADICACION
                            
	FROM FACTURACION.TB_DISTRIBUCIONCUENTAFACTURADO2022 AS DCF

		INNER JOIN LABORATORIO.TB_CABSOLICITUD2022   CS  WITH(NOLOCK) ON CS.IDSOLICITUD = DCF.IDORIGEN  AND CS.ANO = DCF.ANO AND CS.ANULADA = 0
		LEFT JOIN DIMENSIONES.TB_CABSOLICITUD2022  CS1 WITH(NOLOCK) ON CS1.DIMENSION = 17 AND CS1.ANO = CS.ANO AND CS.IDSOLICITUD = CS1.IDSOLICITUD
		--LEFT JOIN DIMENSIONES.VW_URGENTE  UR WITH(NOLOCK)  ON CS.CD3 = UR.IDLISTA
		LEFT JOIN FACTURACION.TB_CABINGRESOS2022  CA  WITH(NOLOCK) ON DCF.IDDOCUMENTO = CA.IDDOCUMENTO 
		LEFT JOIN  FACTURACION.TB_ESTADOFACTURA EF  WITH(NOLOCK) ON EF.IDESTADO = CA.ESTADO
	) C
LEFT JOIN LABORATORIO.TB_DETSOLICITUD2022  DS WITH(NOLOCK) ON DS.IDSOLICITUD = C.IDSOLICITUD AND DS.ANO = C.ANO 
	AND DS.IDPARAMETRO = (SELECT MAX(PA.IDPARAMETRO) FROM LABORATORIO.TB_PARAMETROS PA WHERE C.IDITEM = PA.CODIGOFACTURACION)
LEFT JOIN CARTERA.TB_DETRADICACION2022  DRAD ON DRAD.IDDOCUMENTO = C.IDDOCUMENTO and ANULADO = 0
LEFT JOIN CARTERA.TB_CABRADICACION2022  CRAD on CRAD.IDRADICACION = DRAD.IDRADICACION

WHERE CONVERT(VARCHAR(6), C.FECHASOLICITUD, 112) = 202202 /*AND C.IDPLAN IN ('7281','7280','16712')*/</DTS:VariableValue>
    </DTS:Variable>
  </DTS:Variables>
