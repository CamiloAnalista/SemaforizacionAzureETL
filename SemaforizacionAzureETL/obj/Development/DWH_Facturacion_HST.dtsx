      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{556F0240-B370-4925-A2F1-B6E0693E6F04}"
          SQLTask:SqlStatementSource="WITH DS AS(&#xA;SELECT CONCAT( CONVERT(CHAR(6), DATEADD(MONTH, -1, GETDATE()),112),'01') SK_FECHA, CONVERT(CHAR(6), DATEADD(MONTH, -1, GETDATE()),112) Cierre, CONVERT(CHAR(6), [FechaSolicitud],112)[SK_PeriodoIngreso]&#xA;,SUM(CASE WHEN NUMERODOCUMENTO != 'NA' AND ESTADOFACTURA != 'ANULADA' THEN CANTIDAD * PRECIOITEM ELSE 0 END ) TotalFacturado&#xA;,SUM(CANTIDAD * PRECIOITEM ) TotalIngreso&#xA;,SUM(CASE WHEN NUMERODOCUMENTO = 'NA' AND ESTADOFACTURA != 'ANULADA' THEN CANTIDAD * PRECIOITEM ELSE 0 END ) TotalPendientes&#xA;,NK_Presupuesto&#xA;FROM [SemaforizacionColcan].[fct].[factFacturacion]&#xA;GROUP BY CONVERT(CHAR(6), [FechaSolicitud],112)&#xA;,NK_Presupuesto&#xA;)&#xA;INSERT INTO fct.factFacturacionHst&#xA;SELECT SK_FECHA, Cierre, CONCAT(LEFT([SK_PeriodoIngreso],4),'-',RIGHT([SK_PeriodoIngreso],2)) SK_PeriodoIngreso, TotalFacturado, TotalIngreso, TotalPendientes, NK_Presupuesto FROM DS" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
